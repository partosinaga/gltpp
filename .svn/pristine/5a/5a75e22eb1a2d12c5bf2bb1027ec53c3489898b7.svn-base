html,
body {
    height:100%;
}

@page { margin: 100px 15px -20px 30px; }

body {
    background-color: #fff;
    margin: 15px 15px 15px 15px;
    font: 11px normal Helvetica, Arial, sans-serif;
    color: #000000;
}

table {
    border-collapse:collapse;
    border-spacing:0;
    width: 100%;
    font: 11px normal Helvetica, Arial, sans-serif;
}

a {
    color: #003399;
    background-color: transparent;
    font-weight: normal;
}


#body{
    margin: 0;
}

p.footer{
    text-align: right;
    font-size: 11px;
    border-top: 1px solid #D0D0D0;
    line-height: 32px;
    padding: 0 10px 0 10px;
    margin: 20px 0 0 0;
}

#container{
}

#footer {
    width:100%;
}

.logo_img {
    width:200px;
    margin-left:274px ;
    margin-bottom:10px;
    margin-top:0px;
    align-content: center;
}
