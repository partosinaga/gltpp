#header_container {
    position:fixed;
    height:150px;
    top:-80px;
}

.table_head tr td {
    padding-top:5px;
    vertical-align:top;
    padding-bottom:0px;
}

.table_detail th {
    border: 1px solid #000000;
    padding: 5px;
    background: #f3f3f3;
    vertical-align: top;
}

.table_detail tr{
    page-break-inside: avoid;
}

.table_detail tr td {
    border-right: 1px solid #000000;
    border-left: 1px solid #000000;
    /*border-bottom: 1px solid #000000;*/
    padding: 4px 4px 3px;
    vertical-align: top;
}

.text-center{
    text-align:center;
}

.text-left{
    text-align:left;
}

.text-right{
    text-align:right;
}

.border_bottom{
    border-bottom: 0.5px solid #000000;
}

.bordered {
    border: 1px solid #000000;
}
